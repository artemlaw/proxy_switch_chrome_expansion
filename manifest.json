{
  "name": "Proxy_switch",
  "description": "Интерактивная смена прокси пользователя",
  "version": "0.0.3",
  "manifest_version": 3, 
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [    
    "https://*/*"   
  ], 
  "permissions": ["cookies","storage", "activeTab", "scripting", "nativeMessaging", "gcm", "proxy", "webRequest", "webRequestAuthProvider", "identity", "declarativeNetRequest"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }  
  
}